x=float(input("enter a number"))
print(x*2.2)

NetBalance=float(input("enter the net balance"))
payment=float(input("enter the payment amount"))
d1=int(input("enter a number of days in the billing cycle"))
d2=int(input("enter a number of days before billing cycle the payment was made"))
averageDailyBalance=(NetBalance*d1-payment*d2)/d1
interest=averageDailyBalance*0.0152
print("interest:", interest)

CarAAverageSpeed=float(input("enter the car A average speed:"))
CarBAverageSpeed=float(input("enter the car B average speed:"))
ElapsedTime=float(input("enter the elapsed time:"))
ShortestDistanceBetweenCars=((CarBAverageSpeed*ElapsedTime)**2+(CarAAverageSpeed*ElapsedTime)**2)**0.5
print("ShortestDistanceBetweenCars:", ShortestDistanceBetweenCars)

